<h1>New Form</h1>

<%= form_for @feedbackform,url:feedbackforms_path do |form| %>
  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <br />

  <div class="field">
    <%= form.label :place %>
    <%= form.text_field :place %>
  </div>
  <br />
  <div class="field">
    <%= form.label :age %>
    <%= form.text_field :age %>
  </div>
  <br />



  <div class="actions">
    <%= form.submit "Submit" %>
  </div>
<% end %>
